package com.cdac.gallery.entity;

import java.io.Serializable;

public class Image implements Serializable {
	
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	int id;
	
	String name;
	
	String url;

	public Image(int id, String name, String url) {
		this.id = id;
		this.name = name;
		this.url = url;
	}

}
